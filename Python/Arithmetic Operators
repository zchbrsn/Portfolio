if __name__ == '__main__':
    a = int(input())
    b = int(input())

    if 1 <= a <= 10 ** 10 and 1 <= b <= 10 ** 10:
        sum = a + b
        diff = a - b
        prod = a * b
        divisor = a / b

        print(f"{sum}")
        print(f"{diff}")
        print(f"{prod}")
        print(f"{divisor:.8}")
